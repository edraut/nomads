<h1>Editing item_color</h1>

<% form_for(@item_color) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :item_id %><br />
    <%= f.text_field :item_id %>
  </p>
  <p>
    <%= f.label :color_id %><br />
    <%= f.text_field :color_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @item_color %> |
<%= link_to 'Back', item_colors_path %>
