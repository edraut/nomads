<h1>New campaign_template</h1>

<% form_for(@campaign_template) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :partial %><br />
    <%= f.text_field :partial %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', campaign_templates_path %>
