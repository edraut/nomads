<h1>Editing item_size</h1>

<% form_for(@item_size) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :item_id %><br />
    <%= f.text_field :item_id %>
  </p>
  <p>
    <%= f.label :size_id %><br />
    <%= f.text_field :size_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @item_size %> |
<%= link_to 'Back', item_sizes_path %>
