<div class="column span-19 first last">
	<div class="column span-15 first"><h1>Size Group List</h1></div>
</div>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for size_group in @size_groups %>
  <tr>
    <td><%=h size_group.name %></td>
    <td><%= link_to 'Show', [:admin, size_group] %></td>
    <td><%= link_to 'Edit', edit_admin_size_group_path(size_group) %></td>
    <td><%= link_to 'Delete', [:admin, size_group], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

